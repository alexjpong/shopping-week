<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.parse.starter"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="15" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.parse.starter.ShoppingWeekApplication"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme" >
        <activity
            android:name="Register"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="Login" >
        </activity>
        <activity android:name="ShopClasses" >
        </activity>
         <activity android:name="MyCourses" >
        </activity>
         <activity android:name="CourseCatalog" >
        </activity>
        <activity android:name="com.parse.starter.Homescreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="IndividualCourse"></activity>
    </application>

</manifest>